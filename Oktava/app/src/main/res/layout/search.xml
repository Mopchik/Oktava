        app:layout_constraintStart_toEndOf="@+id/buttonBeginTraining21"
        app:layout_constraintTop_toBottomOf="@+id/buttonBeginTraining16" />

    <Button
        android:id="@+id/buttonBeginTraining17"
        style="@style/ButtonWhite"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="Ска"
        app:layout_constraintStart_toEndOf="@+id/buttonBeginTraining16"
        app:layout_constraintTop_toBottomOf="@+id/buttonBeginTraining13" />

    <Button
        android:id="@+id/buttonBeginTraining18"
        style="@style/ButtonWhite"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="Фолк"
        app:layout_constraintStart_toEndOf="@+id/buttonBeginTraining17"
        app:layout_constraintTop_toBottomOf="@+id/buttonBeginTraining13" />

    <Button
        android:id="@+id/buttonBeginTraining19"
        style="@style/ButtonWhite"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="Шансон"
        app:layout_constraintStart_toEndOf="@+id/buttonBeginTraining18"
        app:layout_constraintTop_toBottomOf="@+id/buttonBeginTraining13" />

    <Button
        android:id="@+id/buttonBeginTraining14"
