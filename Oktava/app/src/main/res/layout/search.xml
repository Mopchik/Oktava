        android:layout_marginStart="20dp"
        android:text="Кантри"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonBeginTraining16" />

    <Button
        android:id="@+id/buttonBeginTraining21"
        style="@style/ButtonWhite"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="Детство"
        app:layout_constraintStart_toEndOf="@+id/buttonBeginTraining20"
        app:layout_constraintTop_toBottomOf="@+id/buttonBeginTraining16" />

    <Button
        android:id="@+id/buttonBeginTraining22"
